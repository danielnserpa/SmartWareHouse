<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ncirl.formcontrollers.Controller">
    <children>
        <Button fx:id="showRobotStatusButton" layoutX="27.0" layoutY="22.0" mnemonicParsing="false" onAction="#showRobotStatusButtonClick" prefHeight="26.0" prefWidth="160.0" text="Show Robot Status" />
        <Button fx:id="showStorageStatusButton" layoutX="208.0" layoutY="22.0" mnemonicParsing="false" onAction="#showStorageStatusButtonClick" prefHeight="26.0" prefWidth="160.0" text="Show Storage Status" />
        <Button fx:id="showTemperatureButton" layoutX="389.0" layoutY="22.0" mnemonicParsing="false" onAction="#showTemperatureButtonClick" prefHeight="26.0" prefWidth="160.0" text="Show Temperature" />
        <Label fx:id="robotStatusLabel" layoutX="20.0" layoutY="65.0" prefHeight="158.0" prefWidth="200.0" />
        <Label fx:id="storageStatusLabel" layoutX="200.0" layoutY="65.0" prefHeight="146.0" prefWidth="200.0" />
        <Label fx:id="temperatureLabel" layoutX="389.0" layoutY="65.0" prefHeight="147.0" prefWidth="352.0" />
    </children>
</AnchorPane>
