<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="587.0" prefWidth="832.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.ncirl.formcontrollers.UnaryController">
    <Button fx:id="showRobotStatusButtonClick" layoutX="14.0" layoutY="22.0" mnemonicParsing="false"
            onAction="#showRobotStatusButtonClick" prefHeight="72.0" prefWidth="212.0" text="Show Robot Status"/>
    <Button fx:id="showStorageStatusButtonClick" layoutX="300.0" layoutY="22.0" mnemonicParsing="false"
            onAction="#showStorageStatusButtonClick" prefHeight="72.0" prefWidth="213.0" text="Show Storage Status"/>
