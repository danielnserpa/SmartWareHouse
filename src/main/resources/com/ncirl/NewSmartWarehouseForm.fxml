    <Label fx:id="robotStatusLabel" layoutX="14.0" layoutY="96.0" prefHeight="149.0" prefWidth="160.0"/>
    <Label fx:id="storageStatusLabel" layoutX="300.0" layoutY="100.0" prefHeight="140.0" prefWidth="190.0"/>
    <Label fx:id="temperatureLabel" layoutX="569.0" layoutY="101.0" prefHeight="140.0" prefWidth="248.0"/>
    <ImageView fitHeight="207.0" fitWidth="438.0" layoutX="178.0" layoutY="424.0" pickOnBounds="true"
